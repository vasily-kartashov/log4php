{
  "name": "vasily-kartashov/log4php",
  "description": "A versatile logging framework for PHP",
  "keywords": ["log", "logging", "php"],
  "homepage": "http://logging.apache.org/log4php/",
  "type": "library",
  "license": "Apache-2.0",
  "autoload": {
    "psr-4": {
      "Log4Php\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/"
    ]
  },
  "require": {
    "php": ">=7.0",
    "psr/log": ">=1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6 || ^7",
    "jakub-onderka/php-parallel-lint": "@stable",
    "phpstan/phpstan": "@stable",
    "vimeo/psalm": "@stable",
    "squizlabs/php_codesniffer": "@stable"
  }
}
