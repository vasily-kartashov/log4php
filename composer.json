{
  "name": "vasily-kartashov/log4php",
  "description": "A versatile logging framework for PHP",
  "keywords": ["log", "logging", "php"],
  "homepage": "http://logging.apache.org/log4php/",
  "type": "library",
  "license": "Apache-2.0",
  "version": "4.0",
  "autoload": {
    "classmap": [
      "src/main/Bridges"
    ],
    "psr-4": {
      "Log4Php\\": "src/main/Log4Php"
    }
  },
  "require": {
    "php": ">=7.0",
    "psr/log": ">=1.0"
  },
  "require-dev": {
    "jakub-onderka/php-parallel-lint": "0.9.*",
    "phpstan/phpstan": "0.6.*",
    "phpunit/phpunit": ">=6",
    "squizlabs/php_codesniffer": ">=2"
  }
}
